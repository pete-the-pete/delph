{
  "manifest_version": 2,
  "name": "delphi",
  "description": "Personal history analytics",
  "version": "0.0.0",
  "icons": {
    "128": "chrome-icon-lg.png"
  },
  "browser_action": {
    "default_icon": "chrome-icon.png"
  },
   "background": {
    "scripts": ["chrome-delphi.js"],
    "persistent": true
  },
  "permissions": [
    "history",
    "tabs"
  ]
}